    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug V2Ray (Skip Go Version Check)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main",
            "args": ["-config", "${workspaceFolder}/deploy/config.json"],
            "env": {
                "GO111MODULE": "on"
            },
            "showLog": true,
            "dlvFlags": ["--check-go-version=false"]
        }
    ]
}